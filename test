source variables

echo "" >> ${LOG} echo
echo "Testing Gameplex Software tools files..." >> ${LOG} echo
echo "checking that all ${FILECT} required files are present..." | xargs >> ${LOG} echo

sleep 2

MISSINGFILES=$((6-$FILECT))

if [ $MISSINGFILES -gt 0 ]
then
    echo "Missing ${MISSINGFILES} file(s)" >> ${LOG} echo
else
    echo "Gameplex Software developer tools is intact!" >> ${LOG} echo
fi
echo ""